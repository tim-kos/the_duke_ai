;*******************************************
;*******************************************
;WHEN TO TRAIN UNITS


(defrule
	(true)
=>
	(set-goal ABLE-TO-TRAIN-UNITS NO)
)


(defrule
	(current-age == feudal-age)
	(up-research-status c: castle-age < research-pending)
	(goal RUSH-CONTROL RUSHING)
	(food-amount > 80) ;leave food for villager creation
	(game-time < 2100)
	(military-population < MILITARY-POPULATION-MAX)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)

(defrule
	(goal RUSH-CONTROL RUSHING)
	(current-age == castle-age)
	(or
		(or
			(goal G-PRIMARY-UNIT knight-line)
			(goal G-PRIMARY-UNIT eagle-warrior-line)
		)
		(and
			(not (research-available ri-hand-cart))
			(building-type-count-total town-center > 1)
		)
	)
	(food-amount > 100) ;leave food for villagers
	(military-population < MILITARY-POPULATION-MAX)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)

#load-if-defined DEATH-MATCH
(defrule
	(food-amount >= 180)
	(military-population < MILITARY-POPULATION-MAX)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)
#end-if

(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(goal MAP-KIND LAND-MAP)
	(soldier-count < SEVENTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS YES)
)

(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(goal MAP-KIND LAND-MAP)
	(soldier-count >= SEVENTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS NO)
)



(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(goal MAP-KIND RIVERS-MAP)
	(soldier-count < FOURTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS YES)
)



(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(goal MAP-KIND RIVERS-MAP)
	(soldier-count >= FOURTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS NO)
)



(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(or
		(goal MAP-KIND WATER-MAP)
		(goal MAP-KIND ISLAND-MAP)
	)
	(soldier-count < THIRTY-FIVE-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS YES)
)



(defrule
	(not (goal MAIN-SHIP-BUILDER YES))
	(or
		(goal MAP-KIND WATER-MAP)
		(goal MAP-KIND ISLAND-MAP)
	)
	(soldier-count >= THIRTY-FIVE-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS NO)
)



(defrule
	(goal MAIN-SHIP-BUILDER YES)
	(soldier-count < THIRTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS YES)
)



(defrule
	(goal MAIN-SHIP-BUILDER YES)
	(soldier-count >= THIRTY-PERCENT-POP)
=>
	(set-goal ABLE-TO-TRAIN-LAND-UNITS NO)
)

(defrule
	(true)
=>
	(set-goal SAVE-FOR-RESEARCH NO)
	(disable-self)
)

(defrule
	(current-age >= castle-age)
	(current-age-time > 120)
	(not (goal RUSH-CONTROL RUSHING))
=>
	(enable-timer T-SAVE-FOR-RESEARCH 1)
	(disable-self)
)

(defrule
	(game-time > 5400) ;if the game is 1.5 hour old, we assume we have researched the most important stuff
	(goal SAVE-FOR-RESEARCH YES)
	(goal ALL-ECONOMY-TECH-DONE YES)
=>
	(disable-timer T-SAVE-FOR-RESEARCH)
	(set-goal SAVE-FOR-RESEARCH NO)
	(chat-local-to-self "Spend resources on units because the game lasts long")
)

(defrule
	(timer-triggered T-SAVE-FOR-RESEARCH)
	(goal SAVE-FOR-RESEARCH NO)
	(game-time <= 5400)
	(military-population > FIVE-PERCENT-POP)
=>
	(set-goal SAVE-FOR-RESEARCH YES)
	(enable-timer T-SAVE-FOR-RESEARCH 180) ;save resources for 3 minutes
	(chat-local-to-self "Save resources for research for 3min")
)

(defrule
	(timer-triggered T-SAVE-FOR-RESEARCH)
	(goal SAVE-FOR-RESEARCH NO)
	(game-time > 5400) ;In late game, only save for 1min. To save on wood to to build more military buildings and delete the last technologies
	(military-population > FIVE-PERCENT-POP)
=>
	(set-goal SAVE-FOR-RESEARCH YES)
	(enable-timer T-SAVE-FOR-RESEARCH 60) ;save resources for 1 min
	(chat-local-to-self "Save resources for research for 1min")
)

