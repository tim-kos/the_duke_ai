;If we have just started researching to the imperial-age, start saving for research right away
;to get the most important upgrades done.
(defrule
	(up-research-status c: imperial-age >= research-pending)
	;(military-population > FIVE-PERCENT-POP)
=>
	(disable-timer T-SAVE-FOR-RESEARCH)
	(set-goal SAVE-FOR-RESEARCH YES)
	(enable-timer T-SAVE-FOR-RESEARCH 300) ;save 5 minutes for important upgrades
	(chat-local-to-self "Save resources for 5min for early upgrades in imperial age")
	(disable-self)
)

(defrule
	(timer-triggered T-SAVE-FOR-RESEARCH)
	(goal SAVE-FOR-RESEARCH YES)
	(goal ALL-ECONOMY-TECH-DONE YES)
=>
	(disable-timer T-SAVE-FOR-RESEARCH)
	(set-goal SAVE-FOR-RESEARCH NO)
	(enable-timer T-SAVE-FOR-RESEARCH 180) ;spend resources for 3 minutes
	(chat-local-to-self "Spend resources for 3min")
)

(defrule
	(goal SAVE-FOR-RESEARCH NO)
	(goal ABLE-TO-TRAIN-UNITS NO)
	(current-age >= castle-age)
	(goal ALL-ECONOMY-TECH-DONE YES)
	(building-type-count-total town-center > 2)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)

(defrule
	(goal SAVE-FOR-RESEARCH NO)
	(goal ABLE-TO-TRAIN-UNITS NO)
	(current-age >= imperial-age)
	(goal ALL-ECONOMY-TECH-DONE YES)
	(building-type-count-total town-center > 2)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)

(defrule
	(goal UNDER-ATTACK YES)
=>
	(set-goal ABLE-TO-TRAIN-UNITS YES)
)

;Make sure we can get to 100 wood to build a new lumber camp
(defrule
	(dropsite-min-distance wood > 7)
	(wood-amount < 150)
=>
	(set-goal ABLE-TO-TRAIN-UNITS NO)
)

;Save resources for an earlier imperial-age
(defrule
	(military-population > FIVE-PERCENT-POP)
	(up-research-status c: imperial-age < research-pending)
	(goal RUSH-CONTROL NO)
=>
	(set-goal ABLE-TO-TRAIN-UNITS NO)
)

